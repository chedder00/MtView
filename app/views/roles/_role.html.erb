<li>
  <%=  "#{role.name}" %>
  <% if admin? %>
    <%= link_to "edit", edit_role_path(role), class: "admin" %>    
    <%= link_to "delete", role, method: :delete, 
                        data: { confirm: "Delete user #{role.name}?" },
                        class: "admin" %>    
  <% end %>
</li>