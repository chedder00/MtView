<li>
  <%= task.updated_at.strftime("%m/%d/%y") %> - 
  <%= link_to task.name, task %>
    <%= link_to "edit", edit_task_path(task), class: "admin" %>
  <% if administrator? %>    
    <%= link_to "delete", task, method: :delete, 
                        data: { confirm: "Delete task #{task.name}?" },
                        class: "admin" %>
  <% end %>
</li>