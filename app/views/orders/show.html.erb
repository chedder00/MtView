<% render 'shared/page_title' %>
<h1><%= "#{@page_heading}" %></h1>
<br />

<div class="row">

  <div class="col-md-6 col-md-offset-3">
    <h2 class="section">Current items in order</h2>
    <% will_paginate @items %>
    <ul class="index" %>
      <%= render @items %>
    </ul>    
    <%= render 'items/form', url: user_order_items_path(order_id: @order.id) %>
  </div>
</div>